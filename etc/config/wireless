
config wifi-device 'radio0'
	option type 'mac80211'
	option hwmode '11g'
	option path '10180000.wmac'
	option channel 'auto'
	option txpower '20'
	option htmode 'HT40'
	option country 'DE'

config wifi-iface
	option device 'radio0'
	option mode 'ap'
	option encryption 'none'
	option ssid 'Deutsch lernen'
	option network 'wlan0'

